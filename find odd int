#include <vector>

int findOdd(const std::vector<int>& numbers){
  
  int count = 0;
  
  for(int i = 0; i < numbers.size(); i++){
    
    for (int j = 0; j < numbers.size(); j++){
      
      if ( numbers[i] == numbers[j]){
        
        count++;
      }        
            
    } 
    if(count % 2 != 0){
      
      return numbers[i];
    
    }
    
  }
  
}
